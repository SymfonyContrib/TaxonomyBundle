{
    "name": "symfonycontrib/taxonomy-bundle",
    "type": "symfony-bundle",
    "description": "Symfony taxonomy management for content organization. (tags, categories, groups, types, etc.)",
    "keywords": ["Symfony", "bundle", "taxonomy", "tags", "categories"],
    "homepage": "http://symfonycontrib.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Doucette",
            "email": "dragonwize@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "symfony/symfony":                   "^3.0",
        "doctrine/orm":                      "^2.5",
        "doctrine/doctrine-bundle":          "^1.6",
        "stof/doctrine-extensions-bundle":   "^1.2",
        "symfonycontrib/confirm-bundle":     "^2.0",
        "components/tabledrag":              "^0.5"
    },
    "autoload": {
        "psr-4": {
            "SymfonyContrib\\Bundle\\TaxonomyBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
