{% extends "TaxonomyBundle::layout.html.twig" %}

{% block page_title %}{{ vocabulary.label ? 'Edit' : 'Add' }} {{ vocabulary.label }} Vocabulary{% endblock %}

{% block content %}
    {{ form_start(form) }}
    {{ form_row(form.label) }}
    {{ form_row(form.desc) }}
    {{ form_widget(form.save) }}
    <a href="{{ cancel_url }}" class="cancel-btn">Cancel</a>
    {{ form_end(form) }}
{% endblock %}
